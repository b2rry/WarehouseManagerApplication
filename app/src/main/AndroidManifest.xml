<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.WarehouseManager"
        tools:targetApi="31">
        <activity
            android:name=".CreateDispatchEventActivity"
            android:exported="false" />
        <activity
            android:name=".CreateSupplyItemActivity"
            android:exported="false" />
        <activity
            android:name=".LogBookActivity"
            android:exported="false"
            android:label="@string/log_book_activity_button" />
        <activity
            android:name=".AboutProgramActivity"
            android:exported="false"
            android:label="@string/about_program_button" />
        <activity
            android:name=".SupplyListActivity"
            android:exported="false"
            android:label="@string/supply_list_activity_button" />
        <activity
            android:name=".SupplyItemActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>